<h1><%= t('.users') %></h1>

<table>
  <thead>
    <tr>
      <th><%= User.human_attribute_name 'email' %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= link_to t('devise.shared.links.show'), user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @users %>
<%= link_to t('.books'), books_path %>
<%= link_to t('devise.shared.links.sign_out'), destroy_user_session_path, method: :delete %>
